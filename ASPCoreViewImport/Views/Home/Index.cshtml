@model List<Student>

@{
    ViewData["Title"] = "Index";
    
}

<h1>Index</h1>

<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Gender</th>
    </tr>
    @{
        foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Gender</td>
            </tr>

        }
    }
</table>



